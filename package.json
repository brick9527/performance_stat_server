{
  "name": "koa-template",
  "version": "0.2.0",
  "description": "A project template based on koa",
  "main": "src/index.js",
  "scripts": {
    "lint": "node_modules/.bin/eslint src --cache --exit-on-fatal-error",
    "dev": "cross-env NODE_ENV=develop node src/index.js",
    "start": "cross-env NODE_ENV=production node src/index.js",
    "commitmsg": "validate-commit-msg",
    "refresh": "node bin/refresh_stat.js"
  },
  "pre-commit": [
    "lint",
    "commitmsg"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brick9527/koa-template.git"
  },
  "keywords": [
    "template",
    "koa"
  ],
  "author": "brick9527",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brick9527/koa-template/issues"
  },
  "homepage": "https://github.com/brick9527/koa-template#readme",
  "dependencies": {
    "@koa/router": "^10.1.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.0",
    "joi": "^17.4.2",
    "js-yaml": "^4.1.0",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "koa-compose": "^4.1.0",
    "koa-helmet": "^6.1.0",
    "koa-session": "^6.2.0",
    "koa2-cors": "^2.0.6",
    "lodash": "^4.17.21",
    "log4js": "^6.4.4",
    "mongoose": "^5.13.7",
    "redis": "^3.1.2"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^7.0.1",
    "pre-commit": "^1.2.2",
    "prettier-eslint": "^13.0.0",
    "validate-commit-msg": "^2.14.0"
  }
}
